{
  "name": "storybook-addon-badges",
  "version": "1.0.2",
  "description": "Add badges to your stories",
  "keywords": [
    "storybook-addons",
    "layout",
    "organize",
    "badges"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tetarchus/storybook-addon-badges.git"
  },
  "license": "MIT",
  "author": "tetarchus <contact@tetbot.app>",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./manager": "./dist/manager.js",
    "./package.json": "./package.json"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "*.js",
    "*.d.ts"
  ],
  "workspaces": [
    "docs"
  ],
  "scripts": {
    "build": "tsup",
    "build-storybook": "storybook build",
    "build:watch": "npm run build -- --watch",
    "prerelease": "zx scripts/prepublish-checks.js",
    "release": "npm run build && auto shipit",
    "start": "run-p build:watch \"storybook --quiet --no-open\"",
    "storybook": "storybook dev -p 6006",
    "test": "vitest",
    "tsc": "tsc"
  },
  "dependencies": {
    "@storybook/icons": "1.2.12"
  },
  "devDependencies": {
    "@auto-it/all-contributors": "11.2.1",
    "@auto-it/conventional-commits": "11.2.1",
    "@auto-it/first-time-contributor": "11.2.1",
    "@storybook/addon-essentials": "8.3.5",
    "@storybook/addon-interactions": "8.3.5",
    "@storybook/addon-links": "8.3.5",
    "@storybook/blocks": "8.3.5",
    "@storybook/react": "8.3.5",
    "@storybook/react-vite": "8.3.5",
    "@storybook/test": "8.3.5",
    "@storybook/theming": "8.3.5",
    "@types/node": "20.16.10",
    "@types/react": "18.3.11",
    "@types/react-dom": "18.3.0",
    "@vitejs/plugin-react": "4.3.2",
    "auto": "11.2.1",
    "boxen": "8.0.1",
    "dedent": "1.5.3",
    "npm-run-all2": "6.2.3",
    "prettier": "3.3.3",
    "prettier-plugin-packagejson": "2.5.3",
    "prettier-plugin-tailwindcss": "0.6.8",
    "prompts": "2.4.2",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "storybook": "8.3.5",
    "tsup": "8.3.0",
    "typescript": "5.6.2",
    "vite": "5.4.8",
    "vite-tsconfig-paths": "5.0.1",
    "vitest": "2.1.2",
    "zx": "8.1.8"
  },
  "peerDependencies": {
    "storybook": "^8.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "bundler": {
    "exportEntries": [
      "src/index.ts"
    ],
    "managerEntries": [
      "src/manager.tsx"
    ]
  },
  "storybook": {
    "displayName": "Badges",
    "supportedFrameworks": [
      "react",
      "vue",
      "angular",
      "web-components",
      "ember",
      "html",
      "svelte",
      "preact",
      "react-native"
    ],
    "icon": "https://raw.githubusercontent.com/tetarchus/storybook-addon-badges/main/.github/images/icon.svg"
  }
}
