{
  "name": "storybook-addon-badges",
  "version": "2.0.0",
  "description": "Add badges to your stories",
  "keywords": [
    "storybook-addons",
    "layout",
    "organize",
    "badges"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tetarchus/storybook-addon-badges.git"
  },
  "license": "MIT",
  "author": "tetarchus <contact@tetbot.app>",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./manager": "./dist/manager.js",
    "./package.json": "./package.json"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "*.js",
    "*.d.ts"
  ],
  "workspaces": [
    "docs"
  ],
  "scripts": {
    "build": "tsup",
    "build-storybook": "storybook build",
    "build:watch": "npm run build -- --watch",
    "lint": "eslint --flag unstable_ts_config .",
    "prerelease": "zx scripts/prepublish-checks.js",
    "release": "npm run build && auto shipit",
    "start": "run-p build:watch \"storybook -- --no-open\"",
    "start:all": "run-p start start:docs",
    "start:docs": "npm run start -w docs",
    "storybook": "storybook dev -p 6006",
    "test": "vitest",
    "tsc": "tsc",
    "validate": "run-p tsc lint \"test -- --no-watch\""
  },
  "dependencies": {
    "@storybook/icons": "1.2.12"
  },
  "devDependencies": {
    "@auto-it/all-contributors": "11.2.1",
    "@auto-it/conventional-commits": "11.3.0",
    "@auto-it/first-time-contributor": "11.2.1",
    "@eslint/js": "9.13.0",
    "@storybook/addon-essentials": "8.3.6",
    "@storybook/addon-interactions": "8.3.6",
    "@storybook/addon-links": "8.3.6",
    "@storybook/blocks": "8.3.6",
    "@storybook/react": "8.3.6",
    "@storybook/react-vite": "8.3.6",
    "@storybook/test": "8.3.6",
    "@storybook/theming": "8.3.6",
    "@storybook/types": "8.3.6",
    "@types/eslint__js": "8.42.3",
    "@types/node": "20.17.0",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@vitejs/plugin-react": "4.3.3",
    "auto": "11.2.1",
    "boxen": "8.0.1",
    "dedent": "1.5.3",
    "eslint": "9.13.0",
    "eslint-plugin-react": "7.37.2",
    "eslint-plugin-react-hooks": "5.1.0-rc-fb9a90fa48-20240614",
    "globals": "15.11.0",
    "jiti": "2.3.3",
    "npm-run-all2": "7.0.1",
    "prettier": "3.3.3",
    "prettier-plugin-packagejson": "2.5.3",
    "prettier-plugin-tailwindcss": "0.6.8",
    "prompts": "2.4.2",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "storybook": "8.3.6",
    "tsup": "8.3.0",
    "type-fest": "4.26.1",
    "typescript": "5.6.3",
    "typescript-eslint": "8.11.0",
    "vite": "5.4.10",
    "vite-tsconfig-paths": "5.0.1",
    "vitest": "2.1.3",
    "zx": "8.1.9"
  },
  "peerDependencies": {
    "storybook": "^8.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "bundler": {
    "exportEntries": [
      "src/index.ts"
    ],
    "managerEntries": [
      "src/manager.ts"
    ]
  },
  "overrides": {
    "eslint": "9.13.0"
  },
  "storybook": {
    "displayName": "Badges",
    "supportedFrameworks": [
      "react",
      "vue",
      "angular",
      "web-components",
      "ember",
      "html",
      "svelte",
      "preact",
      "react-native"
    ],
    "icon": "https://raw.githubusercontent.com/tetarchus/storybook-addon-badges/main/.github/images/icon.svg"
  }
}
